ponent":"audio","_isEnabled":true,"_isHidden":false,"_isSnapShot":false,"_isTemplate":false,"_evolveTags":[],"_layout":"left","_courseId":"6314a96193bb5fe15894cafc","_parentId":"632832ef179c4a085332856a","_createdBy":"5f9844dcdba5350f5b1e30e5","_items":[{"_audio":{"mp3":{"_id":"632876bc179c4a085332f1d3","_extension":"mp3","_type":"audio"},"_isExternalLink":false,"alt":"Siobhain Wood Avatar"},"_setCompletionOn":"inview","_id":"6328766e179c4a085332f197","itemIndex":0,"_transcript":{"button":"Transcribe","body":"<p>Hi I am siobhain Wood&nbsp;</p><p>This section provides a little bit about my working history.</p><p>Before finding myself working in the Learning and Development industry I was managing my family retail stores, employing and training sessional staff with daily activities.</p><p>However over the past 16 years I have dedicated myself to working in the development &amp; learning sector and have recently been recognised by the Learning and Performance Institute (LPI )as a fellow which I am immensely proud off.</p><p>At present I work in the law industry as a People Development Specialist in Performance and Technology. Working for a solicitor designing, developing and delivering high-quality blended learning programmes. Working closely with project managers and subject matter experts (SMEs) to reflect the changing needs for the company&rsquo;s digital learning needs, resources and face-to-face workshops, to drive continues improvement across the company.</p><p>Before that I worked for a Business Processing outsourcing company as the Assistant Training manger for the UK segment. We had multiple clients in sectors such as (Banking, financial services, and insurance), E Commerce, Payment Gateway Solutions, Travel and my favourite, Massive Multiplayer Online Gaming).</p><p>Previously to this I have trained people in accounting and client management software, assistive technology, &amp; customer service for the banking sector, were I coordinated, implemented, delivered and evaluating training workshops, achieving all goals and deadlines.</p><p>And that&rsquo;s a little bit about my working history, thanks for listening.</p>"}}],"_createdAt":"2022-09-19T13:09:35.868Z","_updatedAt":"2022-09-19T14:45:33.089Z","_refId":"63286a0f179c4a085332ecd4","__v":0,"_updatedBy":"5f9844dcdba5350f5b1e30e5","instruction":"Listen to Siobhain talking about the work she has been doing over the past few years."},{"_id":"6328788a179c4a085332f4fd","_interactionLabel":{"_location":"before widget"},"_graphic":{"src":{"_id":"632882bd179c4a08533301ee","_extension":"gif","_type":"image"},"srcAdvanced":{"_setImagePerDeviceSize":false}},"_links":{"_isEnabled":false},"_stars":{"_isEnabled":false},"_points":{"_isEnabled":false},"_badges":{"_badgeIds":[]},"_assignment":{"_isEnabled":false,"_isMarked":false,"_isCorrect":false,"feedback":""},"_courseAssessment":{"_isEnabled":false},"_courseAttainment":{"_isEnabled":false},"_hint":{"_isEnabled":false},"_progress":{"_isEnabled":true},"_reflection":{"_isEnabled":false},"_search":{"_searchTagIds":[]},"_stamps":{"_isEnabled":false},"_tags":{"_isEnabled":false,"_tagsList":[]},"_type":"component","_isQuestionType":false,"title":"Gif Title","_isInteractionLabelEnabled":false,"_classes":"","_shouldReplayOnPageRevisit":false,"_component":"gif","_isEnabled":true,"_isHidden":false,"_isSnapShot":false,"_isTemplate":false,"_evolveTags":[],"_layout":"right","_courseId":"6314a96193bb5fe15894cafc","_parentId":"632832ef179c4a085332856a","_createdBy":"5f9844dcdba5350f5b1e30e5","_createdAt":"2022-09-19T14:11:22.147Z","_updatedAt":"2022-09-19T14:55:00.778Z","_refId":"6328788a179c4a085332f4fd","__v":0,"_updatedBy":"5f9844dcdba5350f5b1e30e5"},{"_id":"63287b1e179c4a085332f70f","_interactionLabel":{"_location":"before widget"},"_graphic":{"srcAdvanced":{"_setImagePerDeviceSize":false},"src":{"_id":"63287b2f179c4a085332f760","_extension":"gif","_type":"image"}},"_links":{"_isEnabled":false},"_stars":{"_isEnabled":false},"_points":{"_isEnab